 "html": "<div class=\"row text-center\">\n\t<h3>Cashier Report</h3>\n\t<b>{{frappe.utils.get_datetime(doc.creation).strftime('%d %b %Y')}}</b>\n</div>\n<br>\n<div class=\"row text-left\">\n\t<h4>Payments</h4>\t\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-12\">\n\t\t<table class=\"table table-bordered\">\n\t\t\t<tr>\n\t\t\t\t<th><b>Audit Date</b></th>\n\t\t\t\t<th><b>Reservation</b></th>\n\t\t\t\t<th><b>Folio</b></th>\n\t\t\t\t<th><b>Customer Name</b></th>\n\t\t\t\t<th><b>Account</b></th>\n\t\t\t\t<th><b>Amount</b></th>\n\t\t\t\t<th><b>User ID</b></th>\n\t\t\t</tr>\n\t\t\t{%- for row in doc.cr_payment_transaction -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<td>{{frappe.utils.get_datetime(doc.creation).strftime('%d %b %Y')}}</td>\n\t\t\t\t\t<td>{{row.reservation_id}}</td>\n\t\t\t\t\t<td>{{row.folio_id}}</td>\n\t\t\t\t\t<td>{{row.customer_id}}</td>\n\t\t\t\t\t<td>{{row.account}}</td>\n\t\t\t\t\t<td class=\"text-right\">Rp. {{row.get_formatted('amount')}}</td>\n\t\t\t\t\t<td>{{row.user}}</td>\n\t\t\t\t</tr>\n\t\t\t{%- endfor -%}\n\t\t</table>\n\t</div>\n</div>\n<div class=\"row text-left\">\n\t<h4>Refund</h4>\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-12\">\n\t\t<table class=\"table table-bordered\">\n\t\t\t<tr>\n\t\t\t\t<th><b>Audit Date</b></th>\n\t\t\t\t<th><b>Reservation</b></th>\n\t\t\t\t<th><b>Folio</b></th>\n\t\t\t\t<th><b>Customer Name</b></th>\n\t\t\t\t<th><b>Account</b></th>\n\t\t\t\t<th><b>Amount</b></th>\n\t\t\t\t<th><b>User ID</b></th>\n\t\t\t</tr>\n\t\t\t{%- for row in doc.cr_refund_transaction -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<td>{{frappe.utils.get_datetime(doc.creation).strftime('%d %b %Y')}}</td>\n\t\t\t\t\t<td>{{row.reservation_id}}</td>\n\t\t\t\t\t<td>{{row.folio_id}}</td>\n\t\t\t\t\t<td>{{row.customer_id}}</td>\n\t\t\t\t\t<td>{{row.account}}</td>\n\t\t\t\t\t<td class=\"text-right\">Rp. {{row.get_formatted('amount')}}</td>\n\t\t\t\t\t<td>{{row.user}}</td>\n\t\t\t\t</tr>\n\t\t\t{%- endfor -%}\n\t\t</table>\n\t</div>\n</div>\n<br>\n<div class=\"row text-left\">\n\t<h4>Total</h4>\t\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-8\">\n\t<table class=\"table\">\n\t\t<tr>\n\t\t\t<td class=\"text-left\"><b>Total Payment</b></td>\n\t\t\t<td class=\"text-right\"><b>Rp. {{doc.get_formatted('total_payment')}}</b></td>\t\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td class=\"text-left\"><b>Total Refund</b></td>\n\t\t\t<td class=\"text-right\"><b> - Rp. {{doc.get_formatted('total_refund')}}</b></td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td class=\"text-left\"><b>Total</b></td>\n\t\t\t<td class=\"text-right\"><b>Rp. {{ frappe.format_value((doc.total_payment - doc.total_refund), {\"fieldtype\":\"Currency\"}) }}</b></td>\n\t\t</tr>\n\t</table>\n</div>\n</div>",
