 "html": "{% set company = frappe.get_doc(\"Global Defaults\").default_company %}\n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-left\">\n        <h3>{{company}}</h3>\n    </div>\n    <div class=\"col-xs-6 text-right\">\n        <h3>{{doc.name}}</h3>\n    </div>\n</div>\n<div class=\"row text-center\">\n\t<h3>Cash Remittance</h3>\n</div>\n<br>\n<div class=\"row\">\n\t<div class =\"col-sm-12 text-right\">\n\t\t<table class=\"table table-bordered\">\n\t\t\t<tr>\n\t\t\t\t<td class=\"col-xs-3\"><b>Status: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">{{doc.status}}</td>\n\t\t\t\t<td class=\"col-xs-3\"><b>Time In: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">{{doc.get_formatted('time_in')}}</td>\n\t\t\t</tr>\n\t\t\t<tr>\n\t\t\t\t<td class=\"col-xs-3\"><b>User: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">{{doc.employee_name}}</td>\n\t\t\t\t<td class=\"col-xs-3\"><b>Time Out: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">{{doc.get_formatted('time_out')}}</td>\n\t\t\t</tr>\n\t\t\t<tr>\n\t\t\t\t<td class=\"col-xs-3\"><b>Balance: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">Rp. {{doc.get_formatted('balance')}}</td>\n\t\t\t\t<td class=\"col-xs-3\"><b>Cash Balance: </b></td>\n\t\t\t\t<td class=\"col-xs-3\">Rp. {{doc.get_formatted('cash_balance')}}</td>\n\t\t\t</tr>\n\t\t</table>\n\t</div>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-xs-12\"><br><br></div>\n</div>\n\n<div class=\"row text-center\">\n\t<h4>Payments</h4>\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-8 col-sm-offset-2\">\n\t\t<table class=\"table\">\n\t\t\t{%- for row in doc.payment_detail -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"text-left\">{{row.mode_of_payment}}</td>\n\t\t\t\t\t<td class=\"text-right\">Rp. {{row.get_formatted('amount')}}</td>\n\t\t\t\t</tr>\n\t\t\t{%- endfor -%}\n\t\t</table>\n\t\t<table class=\"table\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"text-left\"><b>Total Payment</b></td>\n\t\t\t\t\t<td class=\"text-right\"><b>Rp. {{doc.get_formatted('total_payment')}}</b></td>\n\t\t\t\t</tr>\n\t\t</table>\n\t</div>\n</div>\n\n<div class=\"row text-center\">\n\t<h4>Refunds</h4>\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-8 col-sm-offset-2\">\n\t\t<table class=\"table\">\n\t\t\t{%- for row in doc.refund_detail -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"text-left\">{{row.type}}</td>\n\t\t\t\t\t<td class=\"text-right\">Rp. {{row.get_formatted('amount')}}</td>\n\t\t\t\t</tr>\n\t\t\t{%- endfor -%}\n\t\t</table>\n\t\t<table class=\"table\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"text-left\"><b>Total Refund</b></td>\n\t\t\t\t\t<td class=\"text-right\"><b>Rp. {{doc.get_formatted('total_refund')}}</b></td>\n\t\t\t\t</tr>\n\t\t</table>\n\t</div>\n</div>\n\n<div class=\"row text-center\">\n\t<h4>Cash Count</h4>\n</div>\n<div class=\"row\">\n\t<div class=\"col-sm-8 col-sm-offset-2\">\n\t\t<table class=\"table\">\n\t\t\t{%- for row in doc.cc_detail -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"col-xs-4 text-left\">Rp. {{row.get_formatted('nominal')}}</td>\n\t\t\t\t\t<td class=\"col-xs-4 text-center\">{{row.qty}}</td>\n\t\t\t\t\t<td class=\"col-xs-4 text-right\">Rp. {{row.get_formatted('amount')}}</td>\n\t\t\t\t</tr>\n\t\t\t{%- endfor -%}\n\t\t</table>\n\t\t<table class=\"table\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"col-xs-4 text-left\"><b>Total Cash Count</b></td>\n\t\t\t\t\t<td class=\"col-xs-4 text-center\"><b>{{doc.total_cash_qty}}</b></td>\n\t\t\t\t\t<td class=\"col-xs-4 text-right\"><b>Rp. {{doc.get_formatted('total_cash_count')}}</b></td>\n\t\t\t\t</tr>\n\t\t</table>\n\t</div>\n</div>",
